{
  "name": "personalization-api",
  "description": "Rule generator server",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/lessthan3/personalization.git"
  },
  "engines": {
    "node": "20.x.x",
    "yarn": ">=1.22.22"
  },
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc --project ./",
    "dev": "nodemon ./src/main.ts",
    "debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register ./src/main.ts\"",
    "start": "node ./dist/main.js",
    "swagger": "ts-node ./src/docs/generate-docs.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --coverage --runInBand",
    "just:coverage": "jest --coverage --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.10.0",
    "axios": "^1.7.5",
    "body-parser": "^1.20.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "gaxios": "^6.7.0",
    "googleapis": "^140.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.21",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "jest": "29.7.0",
    "jest-config": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.4",
    "prettier": "3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.17.0"
  },
  "resolutions": {
    "@types/express": "^4.17.17"
  }
}
